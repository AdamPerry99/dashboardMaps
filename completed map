$(document).ready(function(){
$('#Havering').click(function(){
  $("#havering_chart").show();
  $("#bromley_chart").hide();
  $("#bexley_chart").hide();
  $("#barking_and_dagenham_chart").hide();
  $("#barnet_chart").hide();
  $("#brent_chart").hide();
  $("#croydon_chart").hide();
  $("#ealing_chart").hide();
  $("#enfield_chart").hide();
  $("#greenwich_chart").hide();
  $("#hackney_chart").hide();
  $("#city_of_london_chart").hide();
  $("#haringey_chart").hide();
  $("#harrow_chart").hide();
  $("#hillingdon_chart").hide();
  $("#kensington_chart").hide();
  $("#lambeth_chart").hide();
  $("#lewisham_chart").hide();
  $("#merton_chart").hide();
  $("#newham_chart").hide();
  $("#redbridge_chart").hide();
  $("#richmond_chart").hide();
  $("#southwark_chart").hide();
  $("#sutton_chart").hide();
  $("#tower_hamlets_chart").hide();
  $("#waltham_chart").hide();
  $("#westminster_chart").hide();
  $("#camden_chart").hide();
  $("#hammersmith_chart").hide();
  $("#islington_chart").hide();
